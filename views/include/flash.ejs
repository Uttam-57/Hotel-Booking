<% if( success && success.length ) {%>
<div class="flesh alert alert-success " role="alert" style="margin-top: 6rem ;">
   
    <%= success %>
   <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close" ></button>
</div>
<% }%>
<% if( error && error.length ) {%>
<div class="flesh alert alert-danger " role="alert" style="margin-top: 6rem ;">
   
    <%= error %>
   <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close" ></button>
</div>
<% }%>